<Window x:Class="BatchRename.AddPresetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="AddPresetWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Canvas>
        
        <TextBox x:Name="textbox" Width="200" Height="200" HorizontalAlignment="Left" Canvas.Left="300" VerticalAlignment="Center"></TextBox>
          
        <Button Click="Okbutton_Click" Content ="Ok" x:Name="Okbutton" Width="80" Height="35" Canvas.Left="360" Canvas.Top="259" HorizontalAlignment="Center" VerticalAlignment="Top"></Button>
    </Canvas>
</Window>
