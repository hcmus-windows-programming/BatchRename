<Window x:Class="BatchRename.EditPresetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="EditDefaultRulesWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Canvas>

        <TextBox x:Name="textbox" Width="300" Height="200" HorizontalAlignment="Left" Canvas.Left="250" VerticalAlignment="Center" Canvas.Top="54"></TextBox>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="302" Canvas.Top="291">
            <Button Click="Okbutton_Click" Content ="Ok" x:Name="Okbutton" Width="80" Height="35"  HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,10,10"></Button>
            <Button Click="Cancelbutton_Click" Content ="Cancel" x:Name="Cancelbutton" Width="80" Height="35" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,10,10"></Button>
        </StackPanel>
    </Canvas>
</Window>
